{% extends '::base.html.twig' %}

{% block title %}Ввод/Вывод{% endblock title %}

{% block left_menu %}
    {% include ('AdminWalletBundle::left_menu.html.twig') %}
{% endblock left_menu %}

{% block page_title %}<span class="icon-users"></span>Withdraw USD{% endblock page_title %}

{% block main_content %}
    <div class="wrapper">
        <div class="fluid">
            <!-- Media table -->
            <div class="widget">
                <div class="whead">
                    <span class="titleIcon">
                        <div style="text-align: center">
                            <img src="{{ asset('images/database-table-icon.png') }}" width="16">
                        </div>
                    </span>
                    <h6>Bank Data</h6><div class="clear"></div>
                </div>
                <div id="dyn" class="hiddenpars">
                    <div style="width: 100%; font-size: 14px; padding: 20px; font-weight: bold">
                        <div style="width: 30%; float: left">
                            Bank Name:
                        </div>
                        <div style="width: 70%; color: black">
                            {{ data.bankName }}
                        </div>
                        <br>

                        <div style="width: 30%; float: left">
                            Bank SWIFT Code:
                        </div>
                        <div style="width: 70%; color: black">
                            {{ data.bankSwiftCode }}
                        </div>
                        <br>

                        <div style="width: 30%; float: left">
                            Bank Street:
                        </div>
                        <div style="width: 70%; color: black">
                            {{ data.bankStreet }}
                        </div>
                        <br>

                        <div style="width: 30%; float: left">
                            Bank City:
                        </div>
                        <div style="width: 70%; color: black">
                            {{ data.bankCity }}
                        </div>
                        <br>

                        <div style="width: 30%; float: left">
                            Bank Country:
                        </div>
                        <div style="width: 70%; color: black">
                            {{ data.bankCountry }}
                        </div>
                        <br>

                        <div style="width: 30%; float: left">
                            Bank Province/State:
                        </div>
                        <div style="width: 70%; color: black">
                            {{ data.bankProvinceState }}
                        </div>
                        <br>

                        <div style="width: 30%; float: left">
                            Bank ZIP / Postal Code:
                        </div>
                        <div style="width: 70%; color: black">
                            {{ data.bankZipPostalCode }}
                        </div>
                        <br>

                        <div style="width: 30%; float: left">
                            Bank Account Number:
                        </div>
                        <div style="width: 70%; color: black">
                            {{ data.bankAccountNumber }}
                        </div>
                        <br>

                        <div style="width: 30%; float: left">
                            IBAN:
                        </div>
                        <div style="width: 70%; color: black">
                            {{ data.iban }}
                        </div>
                        <br>

                        <div style="width: 30%; float: left">
                            Currency:
                        </div>
                        <div style="width: 70%; color: black">
                            {{ data.currency }}
                        </div>
                        <br>

                        <div style="width: 30%; float: left">
                            Bank ABA/Routing Code:
                        </div>
                        <div style="width: 70%; color: black">
                            {{ data.bankAbaRoutingCode }}
                        </div>
                        <br>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock main_content %}